{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
        "@components/*": ["src/components/*"],
        "@images/*": ["src/assets/images/*"],
        "@style/*": ["src/style/*"],
        "@pages/*": ["src/pages/*"],
        "@reducers/*": ["src/reducers/*"],
        "@config/*": ["src/configs/*"],
        "@src/*": ["src/*"],
      }
  },
}
